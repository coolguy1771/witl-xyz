    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "rehype-highlight": "^7.0.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "tailwind-merge": "^3.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250204.0",
    "@opennextjs/cloudflare": "0.4.7",
